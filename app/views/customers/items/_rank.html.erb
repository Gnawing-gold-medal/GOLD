

<% all_ranks.last(4).reverse_each do |item| %>
    <div class="item-box mx-2 text-center">
    　<%= link_to customers_item_path(item), class: "link-style" do %>
        <%= attachment_image_tag item, :image, size:"200x160", style:"border-radius:5%" %><br>
        <%= item.name %>
      <% end %><br>
      ￥<%= item.taxin_price.to_s(:delimited) %>
    </div>
<% end %>